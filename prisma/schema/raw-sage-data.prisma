model RawCommandeSage {
  id           String @id @default(uuid())
  DO_Piece     String @unique
  DO_DateLiv   String
  DO_Tiers     String
  DO_Ref       String
  DO_Coord01   String
  DE_No        String
  DO_Imprim    String
  DO_TotalTTC  String
  DO_NetAPayer String
  BCClient     String
  Creneau      String
  Personne     String
  Obs1         String
  Obs2         String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([DO_Piece])
  @@map("raw_commande_sage")
}
