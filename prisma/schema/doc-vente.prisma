model DocVente {
  // Primary Key & Identifiers
  piece   String @id @map("DO_Piece")
  cbPiece String @map("cbDO_Piece")
  guid    String @map("DO_GUID")

  // Document Information
  domaine    String   @map("DO_Domaine")
  type       String   @map("DO_Type")
  date       DateTime @map("DO_Date")
  ref        String   @map("DO_Ref")
  souche     String   @map("DO_Souche")
  period     String   @map("DO_Period")
  heure      String   @map("DO_Heure")
  statut     String   @map("DO_Statut")
  eStatut    String   @map("DO_EStatut")
  docType    String   @map("DO_DocType")
  refExterne String?  @map("DO_RefExterne")
  noWeb      String?  @map("DO_NoWeb")

  // Client/Tiers Information
  tiers         String  @map("DO_Tiers")
  cbTiers       String  @map("cbDO_Tiers")
  numPayeur     String  @map("CT_NumPayeur")
  cbNumPayeur   String  @map("cbCT_NumPayeur")
  numCentrale   String? @map("CT_NumCentrale")
  cbNumCentrale String? @map("cbCT_NumCentrale")
  contact       String? @map("DO_Contact")

  // Financial Information
  devise       String   @map("DO_Devise")
  cours        String   @map("DO_Cours")
  totalHT      Decimal  @map("DO_TotalHT") @db.Decimal(18, 2)
  totalHTNet   Decimal  @map("DO_TotalHTNet") @db.Decimal(18, 2)
  totalTTC     Decimal  @map("DO_TotalTTC") @db.Decimal(18, 2)
  netAPayer    Decimal  @map("DO_NetAPayer") @db.Decimal(18, 2)
  montantRegle Decimal  @map("DO_MontantRegle") @db.Decimal(18, 2)
  escompte     Decimal  @map("DO_Escompte") @db.Decimal(18, 2)
  txEscompte   Decimal  @map("DO_TxEscompte") @db.Decimal(5, 2)
  ecart        Decimal? @map("DO_Ecart") @db.Decimal(18, 2)

  // Tax Information
  taxe1     Decimal @map("DO_Taxe1") @db.Decimal(18, 2)
  typeTaux1 String  @map("DO_TypeTaux1")
  typeTaxe1 String  @map("DO_TypeTaxe1")
  codeTaxe1 String  @map("DO_CodeTaxe1")
  taxe2     Decimal @map("DO_Taxe2") @db.Decimal(18, 2)
  typeTaux2 String  @map("DO_TypeTaux2")
  typeTaxe2 String  @map("DO_TypeTaxe2")
  codeTaxe2 String  @map("DO_CodeTaxe2")
  taxe3     Decimal @map("DO_Taxe3") @db.Decimal(18, 2)
  typeTaux3 String  @map("DO_TypeTaux3")
  typeTaxe3 String  @map("DO_TypeTaxe3")
  codeTaxe3 String  @map("DO_CodeTaxe3")
  tvaDebit  String? @map("DO_TVADebit")

  // Shipping & Delivery
  expedit          String?   @map("DO_Expedit")
  dateLivr         DateTime? @map("DO_DateLivr")
  dateLivrRealisee DateTime? @map("DO_DateLivrRealisee")
  dateExpedition   DateTime? @map("DO_DateExpedition")
  colisage         String?   @map("DO_Colisage")
  typeColis        String?   @map("DO_TypeColis")
  creneauLivraison String?   @map("Créneau de livraison")
  personneContact  String?   @map("Personne à contacter sur place")
  adresseLivraison String?   @map("Adresse liv° / sce à livrer")

  // References & Links
  coNo      String? @map("CO_No")
  cbCoNo    String? @map("cbCO_No")
  deNo      String? @map("DE_No")
  cbDeNo    String? @map("cbDE_No")
  liNo      String? @map("LI_No")
  cbLiNo    String? @map("cbLI_No")
  caNum     String? @map("CA_Num")
  cbCaNum   String? @map("cbCA_Num")
  caNo      String? @map("CA_No")
  cbCaNo    String? @map("cbCA_No")
  caNumIFRS String? @map("CA_NumIFRS")
  cgNum     String? @map("CG_Num")
  cbCgNum   String? @map("cbCG_Num")
  etNo      String? @map("ET_No")
  cbEtNo    String? @map("cbET_No")
  ebNo      String? @map("EB_No")
  cfarNo    String? @map("CFAR_No")
  cbCfarNo  String? @map("cbCFAR_No")
  facNo     String? @map("FAC_No")
  cbFacNo   String? @map("cbFAC_No")

  // Coordinates
  coord01 String? @map("DO_Coord01")
  coord02 String? @map("DO_Coord02")
  coord03 String? @map("DO_Coord03")
  coord04 String? @map("DO_Coord04")

  // Pricing & Conditions
  condition  String? @map("DO_Condition")
  tarif      String? @map("DO_Tarif")
  typeCalcul String? @map("DO_TypeCalcul")
  conversion String? @map("DO_Conversion")

  // Fees & Franco
  typeFrais       String?  @map("DO_TypeFrais")
  valFrais        Decimal? @map("DO_ValFrais") @db.Decimal(18, 2)
  typeLigneFrais  String?  @map("DO_TypeLigneFrais")
  typeFranco      String?  @map("DO_TypeFranco")
  valFranco       Decimal? @map("DO_ValFranco") @db.Decimal(18, 2)
  typeLigneFranco String?  @map("DO_TypeLigneFranco")

  // Status & Workflow
  nbFacture    String? @map("DO_NbFacture")
  blFact       String? @map("DO_BLFact")
  reliquat     String? @map("DO_Reliquat")
  imprim       String? @map("DO_Imprim")
  ventile      String? @map("DO_Ventile")
  transfere    String? @map("DO_Transfere")
  cloture      String? @map("DO_Cloture")
  attente      String? @map("DO_Attente")
  valide       String? @map("DO_Valide")
  statutBAP    String? @map("DO_StatutBAP")
  demandeRegul String? @map("DO_DemandeRegul")
  exclure      String? @map("DO_Exclure")

  // Payment Information
  refPaiement     String? @map("DO_RefPaiement")
  adressePaiement String? @map("DO_AdressePaiement")
  paiementLigne   String? @map("DO_PaiementLigne")

  // Transaction & Accounting
  transaction String? @map("DO_Transaction")
  typeTransac String? @map("DO_TypeTransac")
  regime      String? @map("DO_Regime")
  catCompta   String? @map("N_CatCompta")
  majCpta     String? @map("DO_MajCpta")
  langue      String? @map("DO_Langue")

  // Additional Information
  provenance   String? @map("DO_Provenance")
  motif        String? @map("DO_Motif")
  motifDevis   String? @map("DO_MotifDevis")
  factureElec  String? @map("DO_FactureElec")
  factureFile  String? @map("DO_FactureFile")
  factureFrs   String? @map("DO_FactureFrs")
  cbFactureFrs String? @map("cbDO_FactureFrs")
  pieceOrig    String? @map("DO_PieceOrig")
  cbPieceOrig  String? @map("cbDO_PieceOrig")

  // Subscription (Abonnement)
  abNo        String?   @map("AB_No")
  debutAbo    DateTime? @map("DO_DebutAbo")
  finAbo      DateTime? @map("DO_FinAbo")
  debutPeriod DateTime? @map("DO_DebutPeriod")
  finPeriod   DateTime? @map("DO_FinPeriod")

  // Cashier Information
  noCaissier   String? @map("CO_NoCaissier")
  cbNoCaissier String? @map("cbCO_NoCaissier")
  coffre       String? @map("DO_Coffre")

  // Expedition Information
  expeIle    String? @map("EXPE : ILE")
  expeNoCsnt String? @map("EXPE N° CSNT")

  // Observations & Comments
  obs1 String? @map("OBS. 1")
  obs2 String? @map("OBS. 2")
  obs3 String? @map("OBS. 3")
  obs4 String? @map("OBS. 4")

  // Order Information
  bcClient         String? @map("BC client")
  cdeSaisiePar     String? @map("Cde saisie par")
  cdeTransmissePar String? @map("Cde transmisse par")
  cdeModeReception String? @map("Cde : mode de réception")
  heureDebut       String? @map("Heure début")

  // Service & Other
  mrNo        String? @map("MR_No")
  codeService String? @map("DO_CodeService")

  // System Fields (cb prefix - likely Cbase/internal fields)
  cbProt         Int       @map("cbProt")
  cbMarq         Int       @map("cbMarq")
  cbCreateur     String    @map("cbCreateur")
  cbModification DateTime  @map("cbModification")
  cbReplication  Int       @map("cbReplication")
  cbFlag         Int       @map("cbFlag")
  cbCreation     DateTime  @default(now()) @map("cbCreation")
  cbCreationUser String    @map("cbCreationUser")
  cbHash         String?   @map("cbHash")
  cbHashVersion  Int?      @map("cbHashVersion")
  cbHashDate     DateTime? @map("cbHashDate")
  cbHashOrder    Int?      @map("cbHashOrder")

  @@index([tiers])
  @@index([date])
  @@index([statut])
  @@index([type])
  @@index([numPayeur])
  @@map("raw_doc_vente")
}
